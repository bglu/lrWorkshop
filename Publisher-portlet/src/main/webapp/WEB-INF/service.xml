<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="de.lineas.training.publisher">
    <namespace>PUB</namespace>
    <entity name="Entry" local-service="true" remote-service="false" uuid="true">

    	<!-- PK fields -->

    	<column name="entryId" type="long" primary="true"></column>
    	
    	
    	<column name="txt" type="String"></column>
    	
    	

    	<!-- Group instance -->

    	<column name="groupId" type="long"></column>

    	<!-- Audit fields -->
    	
    	<finder return-type="Collection" name="GroupId">
    		<finder-column name="groupId"></finder-column>
    	</finder>

    	
    </entity>
</service-builder>